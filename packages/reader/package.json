{
  "name": "@prose-reader/core",
  "version": "0.0.36",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "concurrently -n src,types \"yarn start-src\" \"yarn start-types\"",
    "start-src": "NODE_ENV=development webpack build --config ./webpack.config.js --watch",
    "start-types": "yarn tsc -w --declaration --noEmit false --emitDeclarationOnly",
    "build": "NODE_ENV=production webpack build --config ./webpack.config.js",
    "build:prod": "rm -rf dist && yarn build",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@prose-reader/shared": "^0.0.4"
  },
  "peerDependencies": {
    "rxjs": "*"
  },
  "devDependencies": {
    "terser-webpack-plugin": "^5.2.5",
    "webpack-bundle-analyzer": "^4.5.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.3.0",
    "html-webpack-plugin": "^5.5.0",
    "ts-loader": "^9.2.6",
    "typescript": "*",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.1"
  },
  "gitHead": "a005ccf2d302179a488d3f5e79c1f043d3ce29de"
}
