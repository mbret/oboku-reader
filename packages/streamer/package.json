{
  "name": "@prose-reader/streamer",
  "version": "0.0.34",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "concurrently -n src,types \"yarn start-src\" \"yarn start-types\"",
    "start-src": "NODE_ENV=development webpack build --config ./webpack.config.js --watch",
    "start-types": "yarn tsc -w --preserveWatchOutput --declaration --noEmit false --emitDeclarationOnly",
    "build": "concurrently -n src,types \"yarn build-src\" \"yarn build-types\"",
    "build-src": "NODE_ENV=development webpack build --config ./webpack.config.js",
    "build-types": "yarn tsc --declaration --noEmit false --emitDeclarationOnly",
    "build:prod": "NODE_ENV=production webpack build --config ./webpack.config.js && yarn build-types",
    "lint": "prettier -w . && eslint --fix . --ext .ts,.tsx,.js,.jsx",
    "test": "jest"
  },
  "dependencies": {
    "@prose-reader/shared": "^0.0.6",
    "@types/xmldoc": "^1.1.5",
    "buffer": "6.0.3",
    "xmldoc": "^1.1.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.4.0",
    "typescript": "*",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "gitHead": "a005ccf2d302179a488d3f5e79c1f043d3ce29de"
}
