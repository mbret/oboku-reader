{
  "name": "@prose-reader/react",
  "version": "1.15.0",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "concurrently -n src,types \"yarn start-src\" \"yarn start-types\"",
    "start-src": "NODE_ENV=development webpack build --config ./webpack.config.js --watch",
    "start-types": "yarn tsc -w --preserveWatchOutput --declaration --noEmit false --emitDeclarationOnly",
    "build": "concurrently -n src,types \"yarn build-src\" \"yarn build-types\"",
    "build-src": "NODE_ENV=development webpack build --config ./webpack.config.js",
    "build-types": "yarn tsc --declaration --noEmit false --emitDeclarationOnly",
    "build:prod": "NODE_ENV=production webpack build --config ./webpack.config.js && yarn build-types",
    "lint:read": "prettier --check . && eslint .",
    "lint:write": "prettier --write . && eslint --fix .",
    "tsc": "tsc"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@prose-reader/core": "^1.15.0"
  },
  "gitHead": "fe0faae4e663ef215fe9ecb047076aab5d2dfd4a"
}
