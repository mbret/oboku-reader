{
  "name": "@oboku/reader-react",
  "version": "0.0.16",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "concurrently -n src,types \"yarn start-src\" \"yarn start-types\"",
    "start-src": "NODE_ENV=development webpack build --config ./webpack.config.js --watch",
    "start-types": "yarn tsc -w --declaration --noEmit false --emitDeclarationOnly",
    "build-types": "yarn tsc --declaration --noEmit false --emitDeclarationOnly",
    "build": "yarn build-types && NODE_ENV=development webpack build --config ./webpack.config.js",
    "build:prod": "rm -rf dist && NODE_ENV=production webpack build --config ./webpack.config.js",
    "lint": "echo",
    "test": "echo"
  },
  "peerDependencies": {
    "@oboku/reader": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "*",
    "react-dom": "*"
  },
  "gitHead": "a005ccf2d302179a488d3f5e79c1f043d3ce29de"
}
